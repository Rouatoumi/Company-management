<form action="{{path('service_new')}}" method="post">
	<div class="card-body">
		<div class="form-group">
			<label for="exampleInputtitre">Titre</label>
			<input type="text" name="titre" class="form-control" id="exampleInputtitre" placeholder="Enter titre">
		</div>
		<div class="form-group">
			<label for="exampleInputdescription">Description
			</label>
			<textarea name="description" class="form-control" id="" cols="30" rows="10"></textarea>
		</div>
		<div class="form-group">
			<label for="exampleInputtelephone">Prix
			</label>
			<input type="number" class="form-control" name="prix" id="">
